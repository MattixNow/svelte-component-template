{
  "name": "svelte-component",
  "description": "This is atemplate for building",
  "license": "MIT",
  "repository": "https://github.com/YogliB/svelte-component-template",
  "svelte": "src/components/components.module.js",
  "module": "dist/index.min.mjs",
  "main": "dist/index.min.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@testing-library/svelte": "1.7.1",
    "@types/jest": "24.0.15",
    "autoprefixer": "9.6.0",
    "babel-jest": "24.8.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-svelte3": "2.4.0",
    "husky": "2.7.0",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "jest-transform-svelte": "2.0.2",
    "lint-staged": "8.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.17",
    "prettier": "1.18.2",
    "rollup": "1.16.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-livereload": "1.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-svelte": "5.1.0",
    "rollup-plugin-terser": "5.0.0",
    "sirv-cli": "0.4.4",
    "svelte": "3.6.2",
    "svelte-preprocess": "2.14.1"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "browserslist": [
    "defaults"
  ],
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist/index.min.mjs",
    "dist/index.min.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, svelte}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css, html, js, json}": [
      "prettier --write",
      "git add"
    ]
  }
}
