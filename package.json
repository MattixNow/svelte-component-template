{
  "name": "svelte-component",
  "svelte": "src/components/components/components.module.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "version": "0.0.1",
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-svelte3": "1.2.3",
    "husky": "2.4.0",
    "lint-staged": "8.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rollup": "1.14.4",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-livereload": "1.0.0",
    "rollup-plugin-node-resolve": "5.0.1",
    "rollup-plugin-svelte": "5.0.3",
    "rollup-plugin-terser": "5.0.0",
    "sirv-cli": "0.4.3",
    "svelte": "3.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist/index.mjs",
    "dist/index.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, svelte}": [
      "eslint --fix",
      "git add"
    ],
    "*.*": [
      "prettier --write",
      "git add"
    ]
  }
}
